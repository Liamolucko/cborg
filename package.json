{
  "name": "cborg",
  "version": "1.0.0",
  "description": "Minimal CBOR encoder & decoder",
  "main": "cborg.js",
  "type": "module",
  "scripts": {
    "lint": "standard",
    "test:browser": "polendina --page --worker --serviceworker --cleanup test/**/test-*.js",
    "test:node": "c8 --check-coverage --reporter=text mocha test/**/test-*.js",
    "test": "npm run lint && npm run test:node && npm run test:browser",
    "coverage": "c8 --reporter=html --reporter=text mocha test/**/test-*.js && npx st -d coverage -p 8888"
  },
  "repository": {
    "type": "git",
    "url": "https+git://github.com/rvagg/cborg"
  },
  "keywords": [
    "cbor"
  ],
  "author": "Rod <rod@vagg.org> (http://r.va.gg/)",
  "license": "Apache-2.0",
  "devDependencies": {
    "c8": "^7.3.5",
    "chai": "^4.2.0",
    "garbage": "^0.0.0",
    "mocha": "^8.2.1",
    "polendina": "^1.1.0",
    "standard": "^16.0.1"
  },
  "dependencies": {
    "@sindresorhus/is": "^4.0.0"
  },
  "exports": {
    ".": {
      "import": "./cborg.js"
    }
  }
}
